{
  "typeCheckingMode": "basic",
  "venvPath": ".",
  "venv": ".venv",
  "include": ["**/*.py"],
  "exclude": [
    "**/node_modules",
    "**/__pycache__",
    "**/.*",
    "**/venv",
    "**/.venv",
    "**/.venv-tests",
    "alembic"
  ],
  "reportAttributeAccessIssue": "information",
  "extraPaths": ["."],
  "verboseOutput": true,
  "enableExperimentalFeatures": true,
  "useLibraryCodeForTypes": true
}
